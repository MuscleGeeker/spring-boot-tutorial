server:
  port: 8040
eureka:
  instance:
    appname: microservice-gateway-zuul
    instance-id: ${eureka.instance.appname}:${spring.cloud.client.ipAddress}:${server.port}
    prefer-ip-address: true
  client:
    service-url:
      defaultZone: http://user:password@localhost:8761/eureka/
zuul:
  prefix: /api